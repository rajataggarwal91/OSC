                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="ellipse4" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                    <SplineDoubleKeyFrame KeyTime="00:00:00.2000000" Value="-0.184"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:02" Value="-0.184"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Name="timelineScore">
                <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="txtAsterisk" Storyboard.TargetProperty="(UIElement.Opacity)">
                    <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:00.2000000" Value="1"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:02" Value="1"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:02.3000000" Value="0"/>
