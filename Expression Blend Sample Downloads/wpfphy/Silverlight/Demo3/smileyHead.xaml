                <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="txtAsterisk" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)">
                    <SplineDoubleKeyFrame KeyTime="00:00:00.2000000" Value="0"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:01" Value="350"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:02" Value="710"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="canvas" Storyboard.TargetProperty="(UIElement.Opacity)">
                    <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                    <SplineDoubleKeyFrame KeyTime="00:00:01.2000000" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>

        </Canvas.Resources>

        <Canvas.RenderTransform>
            <TransformGroup>
                <!-- This transform conrols the rotation-->
                <RotateTransform x:Name="rotateSprite" Angle="0" CenterX="30" CenterY="30"/>
                <TranslateTransform x:Name="translateTransform" X="0" Y="0"/>
            </TransformGroup>
        </Canvas.RenderTransform>





