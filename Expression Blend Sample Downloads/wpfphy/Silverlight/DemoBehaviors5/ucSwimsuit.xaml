				<pb:PhysicsJointBehavior AngleSpringConstant="10000" AngleSpringEnabled="False" BodyOne="rightThigh" BodyTwo="rightCalf" CollisionGroup="1" AngleLimitLower="-75" AngleLimitUpper="0"/>
